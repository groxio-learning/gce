      legend {
          box-sizing: border-box;
          color: inherit;
          display: table;
          max-width: 100%;
          padding: 0;
          white-space:normal
      }

      textarea {
          overflow:auto
      }

      [type=checkbox], [type=radio] {
          box-sizing: border-box;
          padding:0
